# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET        /                                 controllers.Application.index()
GET        /we-lab3                          controllers.Application.index()
GET        /we-lab3/register                 controllers.Application.register()
GET        /we-lab3/jeopardy                 controllers.Application.game()
GET        /we-lab3/login                    controllers.Application.login()
GET        /we-lab3/logout                   controllers.Application.logout()
GET        /we-lab3/jeopardy/question        controllers.Application.question()
GET        /we-lab3/jeopardy/winner          controllers.Application.winner()

# Map static resources from the /public folder to the /assets URL path
GET        /assets/*file                     controllers.Assets.at(path="/public", file)
